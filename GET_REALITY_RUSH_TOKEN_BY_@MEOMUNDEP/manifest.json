{
  "manifest_version": 3,
  "name": "Get Reality Rush tokens by @MeoMunDep",
  "version": "1.0.0",
  "description": "Retrieve token for Reality Rush by @MeoMunDep",
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": ["https://*.telegram.org/*", "*://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/16.png",
    "24": "icons/24.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html"
  }
}
